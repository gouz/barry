{
  "name": "barry",
  "version": "1.0.0",
  "description": "Calc barycenter",
  "scripts": {
    "preparcel": "rm -rf dist .parcel-cache  docs",
    "parcel": "parcel $RUN src/index.pug",
    "build": "RUN=build npm run parcel -- --no-cache --no-source-maps --dist-dir docs --public-url \"./\"",
    "watch": "RUN=watch npm run parcel",
    "dev": "RUN=serve npm run parcel -- --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gouz/barry.git"
  },
  "author": "Sylvain Gougouzian",
  "license": "MIT",
  "devDependencies": {
    "@parcel/compressor-brotli": "2.2.1",
    "@parcel/compressor-gzip": "2.2.1",
    "@parcel/packager-raw-url": "2.2.1",
    "@parcel/plugin": "2.2.1",
    "@parcel/reporter-dev-server": "2.2.1",
    "@parcel/transformer-image": "2.2.1",
    "@parcel/transformer-less": "2.2.1",
    "@parcel/transformer-pug": "2.2.1",
    "@parcel/transformer-webmanifest": "2.2.1",
    "@prettier/plugin-pug": "1.19.1",
    "autoprefixer": "10.4.2",
    "cssnano-preset-advanced": "5.1.10",
    "parcel": "2.2.1",
    "postcss": "8.4.5",
    "tailwindcss": "3.0.15"
  },
  "dependencies": {
    "geoportal-access-lib": "3.0.5",
    "nominatim-geocoder": "0.1.4",
    "ol": "6.12.0"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "targets": {
    "default": {
      "includeNodeModules": true
    }
  }
}
